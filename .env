# Environment configuration for Create React App (CRA)
# Note: Only variables prefixed with REACT_APP_ are exposed to the browser.
# Do NOT put secrets here.

# App
REACT_APP_APP_NAME=BBL
REACT_APP_ENV=development

# API endpoints
REACT_APP_API_BASE_URL = https://space.GigaSpace.org
REACT_APP_PUBLIC_SHARE_BASE = https://gigaspace.org/

# Feature flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_SENTRY=false

# Search
REACT_APP_SEARCH_DEBOUNCE_MS=300

# Build/runtime hints (CRA reads these but does not expose unless prefixed)
# PORT=3000
# BROWSER=none
